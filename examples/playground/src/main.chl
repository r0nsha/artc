fn main() = {
  let me = Person { name: "Ron", age: 26 }
  let me = Person("Ron", 26)
  let me = Person("Ron", age: 26)
  let me = Person(name: "Ron", age: 26)
  print_person(me.name, me.age)
}

fn print_person(name: *str, age: uint) = {
  std.c.printf("%s %d\n".ptr, name.ptr, age)
  ()
}

type Person = struct {
  name: *str,
  age: uint
}

