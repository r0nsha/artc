fn main() = {
  let me = Person { name: "Ron", age: 26 }
  let me = Person("Ron", 26)
  // let me = Person("Ron", age: 26)
  // let me = Person(name: "Ron", age: 26)
  print_person(me.name, foo: 5, age: me.age, foo: 1)
}

fn print_person(name: *str, age: uint, foo: int) = {
  std.c.printf("%s %d %d\n".ptr, name.ptr, foo, age)
  ()
}

type Person = struct {
  name: *str,
  age: uint
}

